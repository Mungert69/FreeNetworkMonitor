#!/bin/sh
value=`cat ../secret`
git pull https://mungert69:$value@github.com/Mungert69/FreeNetworkMonitor
rm ./public/click.freenetworkmonitor*
cp ./downloads/click.freenetworkmonitor.networkmonitormaui-Signed.apk ./public/click.freenetworkmonitor.networkmonitormaui.apk
npm install --omit=dev
npm audit
npm run build
rm -r ./out
cd ../FreeNetworkMonitorBlog
git pull https://mungert69:$value@github.com/Mungert69/FreeNetworkMonitorBlog
npm install 
npm run export
cp -r ./out ../FreeNetworkMonitor/
cd ../FreeNetworkMonitor
docker compose -f docker-compose.yml build
docker push mungert/networkmonitorreact:0.6.20

